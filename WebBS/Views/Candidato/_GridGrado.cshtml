
@using WebBS.Models


<fieldset>
    <div class="panel panel-default">
        <!-- Table -->
        <div class="table-responsive">
            <table class="table table-striped table-bordered table-hover">
                <thead>
                    <tr>
                        <th>Titulo</th>
                        <th>Centro de Estudios</th>
                        <th>Año Graduación</th>
                        <th>Certificado</th>
                        <th>Eliminar</th>

                    </tr>
                </thead>
                @if ((List<RRH_GradoAcademico>)Session["Grados"] != null)
                {
                   
                    foreach (var item in (List<RRH_GradoAcademico>)Session["Grados"])
                    {
                        <tr>
                            <td>
                                @item.Titulo
                            </td>
                            <td>
                                @item.CentroEstudios
                            </td>
                            <td>
                                @item.Anio_graduacion
                            </td>
                            
                            <td></td>
                            <td class="text-right">
                                <a id="CreateCustomer2" href="..\Candidato\EliminarGrado" title="">
                                    <i class="glyphicon glyphicon-remove"></i>
                                </a>

                            </td>

                        </tr>
                    }
                }

            </table>
        </div>
    </div>
</fieldset>

