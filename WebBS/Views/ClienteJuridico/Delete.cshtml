@model WebBS.Models.GCC_CLIENTE

@{
    ViewBag.Title = "DeleteConfirmed";
}

<h2>Confirmar Anulación</h2>

<h3>Seguro que desea anular al Cliente Jurídico?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt hidden="hidden">
            Codigo Cliente
        </dt>

        <dd hidden="hidden">
            @Html.DisplayFor(model => model.Cod_cliente)
        </dd>

        <dt>
            RUC
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Num_doc_identidad)
        </dd>

        <dt>
            Razón Social
        </dt>

        <dd>
            @Html.DisplayFor(model => model.GCC_CLIENTE_JURIDICO.Razon_social)
        </dd>

        <dt>
            Dirección
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Direccion)
        </dd>

        <dt>
            Teléfono
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Telefono)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Correo)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Correo)
        </dd>

        
    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <a href="@Url.Content("~/ClienteJuridico/Index")"><input type="button" value="<< Regresar" class="btn btn-default" /></a>
            <input type="submit" value="Anular" class="btn btn-default" />
        </div>
    }
</div>
